{
  "name": "glencore-test",
  "version": "0.1.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve:build": "yarn http-server build -o",
    "test": "react-scripts test",
    "test:coverage": "yarn test:collect-coverage && yarn serve:coverage",
    "test:collect-coverage": "yarn test --coverage --collectCoverageFrom=src/**/*.{ts,tsx} --collectCoverageFrom=!src/**/{index,*.stories}.{ts,tsx}",
    "serve:coverage": "yarn http-server coverage/lcov-report -o",
    "test:e2e": "yarn start-server-and-test start http://localhost:3000 cypress:run",
    "eject": "react-scripts eject",
    "lint": "yarn eslint ./src/**/*.{ts,tsx}",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headed",
    "run-all": "yarn test:e2e && yarn parallelshell -w \"yarn storybook\" \"yarn test:coverage\" \"yarn start\""
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@types/jest": "24.0.11",
    "@types/node": "11.11.3",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "bootstrap": "^4.3.1",
    "hover.css": "^2.3.2",
    "lodash": "^4.17.11",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router": "^4.4.0",
    "react-router-dom": "^4.4.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "3.3.3333",
    "utility-types": "^3.4.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-info": "^5.0.3",
    "@storybook/addon-knobs": "^5.0.3",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addon-notes": "^5.0.4",
    "@storybook/addon-options": "^5.0.3",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "@storybook/theming": "^5.0.3",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/lodash": "^4.14.123",
    "@types/react-redux": "^7.0.3",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-info": "^4.1.1",
    "@types/storybook__addon-knobs": "^4.0.4",
    "@types/storybook__addon-links": "^3.3.4",
    "@types/storybook__addon-notes": "^4.0.2",
    "@types/storybook__addon-options": "^4.0.1",
    "@types/storybook__addons": "^4.1.0",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-loader": "^8.0.5",
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-typescript": "^0.14.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.11.0",
    "parallelshell": "^3.0.2",
    "prettier": "^1.16.4",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-markdown": "^4.0.6",
    "start-server-and-test": "^1.7.12"
  }
}
